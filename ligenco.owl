@prefix : <http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#> .
@prefix dc: <http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#> .
@prefix gnd: <https://d-nb.info/standards/elementset/gnd#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/#term_> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix fabio: <http://purl.org/spar/fabio/> .
@prefix ligenco: <http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#> .
@base <http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco> .

<http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco> rdf:type owl:Ontology ;
                                                                owl:imports <http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl> ,
                                                                            <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ;
                                                                <http://purl.org/dc/elements/1.1/creator> "Daniele Liberatore"@en ,
                                                                                                          "Matteo Palazzo"@en ;
                                                                <http://purl.org/dc/elements/1.1/date> "2022-09-13" ;
                                                                <http://purl.org/dc/elements/1.1/description> "An ontology used to describe the distribution and production of book genres across the world."@en ;
                                                                <http://purl.org/dc/elements/1.1/title> "Literary Genres by Country"@en ;
                                                                vann:preferredNamespacePrefix "ligenco" ;
                                                                vann:preferredNamespaceUri "http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#" ;
                                                                rdfs:label "LiGenCo"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty ;
                              rdfs:comment "The preferred namespace prefix to use when using terms from this vocabulary in an XML document."@en ;
                              rdfs:isDefinedBy "http://purl.org/vocab/vann/" ;
                              rdfs:label "Preferred Namespace Prefix"@en .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty ;
                           rdfs:comment "The preferred namespace URI to use when using terms from this vocabulary in an XML document."@en ;
                           rdfs:isDefinedBy "http://purl.org/vocab/vann/" ;
                           rdfs:label "Preferred Namespace Uri"@en .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent> rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#belongToMovement
ligenco:belongToMovement rdf:type owl:ObjectProperty ;
                         owl:inverseOf ligenco:hasExponent ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain ligenco:Writer ;
                         rdfs:range ligenco:LiteraryMovement ;
                         rdfs:comment "A writer is born in a specific epoch related to a major literary movement."@en ;
                         rdfs:label "belong to movement"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#bookInMovement
ligenco:bookInMovement rdf:type owl:ObjectProperty ;
                       rdfs:domain ligenco:Book ;
                       rdfs:range ligenco:LiteraryMovement ;
                       owl:propertyChainAxiom ( ligenco:writtenBy
                                                ligenco:belongToMovement
                                              ) ;
                       rdfs:comment "A book written by a writer who belongs to a specific literary movement."@en ;
                       rdfs:label "book in movement"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#contains
ligenco:contains rdf:type owl:ObjectProperty ;
                 owl:inverseOf ligenco:locatedIn ;
                 rdf:type owl:TransitiveProperty ;
                 rdfs:domain ligenco:GeographicalArea ;
                 rdfs:range ligenco:GeographicalArea ;
                 rdfs:comment "A 'Geographical area' contains a 'Geographical area'"@en ;
                 rdfs:label "contains"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasBeenPublished
ligenco:hasBeenPublished rdf:type owl:ObjectProperty ;
                         owl:inverseOf ligenco:publication_of ;
                         rdf:type owl:InverseFunctionalProperty ;
                         rdfs:domain ligenco:Book ;
                         rdfs:range ligenco:BookEdition ;
                         rdfs:comment "A book has been published as a specific book edition."@en ;
                         rdfs:label "has been published"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasBeenPublishedInCountry
ligenco:hasBeenPublishedInCountry rdf:type owl:ObjectProperty ;
                                  rdfs:domain ligenco:Book ;
                                  rdfs:range ligenco:Country ;
                                  owl:propertyChainAxiom ( ligenco:hasBeenPublished
                                                           ligenco:publishedIn
                                                         ) ;
                                  rdfs:comment "A book has been published as a specific book edition in a particular country."@en ;
                                  rdfs:label "has been published in country"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasCollection
ligenco:hasCollection rdf:type owl:ObjectProperty ;
                      owl:inverseOf ligenco:hasPublisher ;
                      rdfs:domain ligenco:Publisher ;
                      rdfs:range ligenco:BookCollection ;
                      rdfs:comment "A publisher which published a book collection."@en ;
                      rdfs:label "has collection"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasExponent
ligenco:hasExponent rdf:type owl:ObjectProperty ,
                             owl:InverseFunctionalProperty ;
                    rdfs:domain ligenco:LiteraryMovement ;
                    rdfs:range ligenco:Writer ;
                    rdfs:comment "A writer belong to a literary movement"@en ;
                    rdfs:label "is exponent"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasPublisher
ligenco:hasPublisher rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain ligenco:BookCollection ;
                     rdfs:range ligenco:Publisher ;
                     rdfs:comment "A book collection published by a publisher."@en ;
                     rdfs:label "has publisher"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#has_been_related_to
ligenco:has_been_related_to rdf:type owl:ObjectProperty ;
                            owl:inverseOf ligenco:relatedTo ;
                            rdfs:domain ligenco:Country ;
                            rdfs:range ligenco:Writer ;
                            rdfs:comment "A 'Country' has been related to a 'Writer'"@en ;
                            rdfs:label "has been related to"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#has_publication
ligenco:has_publication rdf:type owl:ObjectProperty ;
                        owl:inverseOf ligenco:publishedIn ;
                        rdfs:domain ligenco:Country ;
                        rdfs:range ligenco:BookEdition ;
                        rdfs:comment "A book edition has been published in a country"@en ;
                        rdfs:label "has publication"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hosted
ligenco:hosted rdf:type owl:ObjectProperty ;
               owl:inverseOf ligenco:livedIn ;
               rdfs:domain ligenco:Country ;
               rdfs:range ligenco:Writer ;
               rdfs:comment "A 'Country' hosted a 'Writer'"@en ;
               rdfs:label "hosted"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hostsPublisher
ligenco:hostsPublisher rdf:type owl:ObjectProperty ;
                       owl:inverseOf ligenco:setIn ;
                       rdf:type owl:InverseFunctionalProperty ;
                       rdfs:domain ligenco:Country ;
                       rdfs:range ligenco:Publisher ;
                       rdfs:comment "A 'Country' hosts a 'Publisher'"@en ;
                       rdfs:label "hosts publisher"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#isCoevalTo
ligenco:isCoevalTo rdf:type owl:ObjectProperty ,
                            owl:SymmetricProperty ;
                   rdfs:domain ligenco:Book ;
                   rdfs:range ligenco:Book ;
                   rdfs:comment "Two books belong to the same literary movement"@en ;
                   rdfs:label "is coeval to"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#isCompatriotWith
ligenco:isCompatriotWith rdf:type owl:ObjectProperty ,
                                  owl:SymmetricProperty ;
                         rdfs:domain ligenco:Writer ;
                         rdfs:range ligenco:Writer ;
                         rdfs:comment "A 'Writer' isCompatriotWith a 'Writer'"@en ;
                         rdfs:label "is compratiot with"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#livedIn
ligenco:livedIn rdf:type owl:ObjectProperty ;
                rdfs:domain ligenco:Writer ;
                rdfs:range ligenco:Country ;
                rdfs:comment "A writer lived in a country."@en ;
                rdfs:label "lived in"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#livedInTheSameEpochAs
ligenco:livedInTheSameEpochAs rdf:type owl:ObjectProperty ,
                                       owl:SymmetricProperty ;
                              rdfs:domain ligenco:Writer ;
                              rdfs:range ligenco:Writer ;
                              rdfs:comment "Two writer lived in the same epoch"@en ;
                              rdfs:label "lived in the same epoch as"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#locatedIn
ligenco:locatedIn rdf:type owl:ObjectProperty ,
                           owl:TransitiveProperty ;
                  rdfs:domain ligenco:GeographicalArea ;
                  rdfs:range ligenco:GeographicalArea ;
                  rdfs:comment "A geographical area can be located into an another and more restricted geographical area."@en ;
                  rdfs:label "located in"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#overlapsWith
ligenco:overlapsWith rdf:type owl:ObjectProperty ,
                              owl:SymmetricProperty ;
                     rdfs:domain ligenco:LiteraryMovement ;
                     rdfs:range ligenco:LiteraryMovement ;
                     rdfs:comment "A 'Literary movement' overlaps with a 'Literary movement'"@en ;
                     rdfs:label "overlaps with"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publication_of
ligenco:publication_of rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain ligenco:BookEdition ;
                       rdfs:range ligenco:Book ;
                       rdfs:comment "A  'Book edition' is a publication of a 'Book'" ;
                       rdfs:label "publication of" .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#published
ligenco:published rdf:type owl:ObjectProperty ;
                  owl:inverseOf ligenco:publishedBy ;
                  rdf:type owl:InverseFunctionalProperty ;
                  rdfs:domain ligenco:Publisher ;
                  rdfs:range ligenco:BookEdition ;
                  rdfs:comment "A 'Publisher' published a 'Book edition'"@en ;
                  rdfs:label "published"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publishedBy
ligenco:publishedBy rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain ligenco:BookEdition ;
                    rdfs:range ligenco:Publisher ;
                    rdfs:comment "A book edition is published by a publisher."@en ;
                    rdfs:label "published by"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publishedIn
ligenco:publishedIn rdf:type owl:ObjectProperty ;
                    rdfs:domain ligenco:BookEdition ;
                    rdfs:range ligenco:Country ;
                    rdfs:comment "A book edition has been published in a country."@en ;
                    rdfs:label "published in"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#relatedTo
ligenco:relatedTo rdf:type owl:ObjectProperty ;
                  rdfs:domain ligenco:Writer ;
                  rdfs:range ligenco:Country ;
                  rdfs:comment "A writer is related to a country if he is born in this country or if he is an active citizen of it."@en ;
                  rdfs:label "related to"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#relatedToIndividualBook
ligenco:relatedToIndividualBook rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent> ;
                                rdfs:domain ligenco:IndividualBook ;
                                rdfs:range ligenco:Book ;
                                rdfs:label "related to individual book"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#relatedToSequenceBook
ligenco:relatedToSequenceBook rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent> ;
                              rdfs:domain ligenco:SequenceBook ;
                              rdfs:range ligenco:Book ;
                              rdfs:label "related to sequence book"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#seriesHasPublisher
ligenco:seriesHasPublisher rdf:type owl:ObjectProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain ligenco:BookSeries ;
                           rdfs:range ligenco:Publisher ;
                           rdfs:comment "A book series published by a publisher."@en ;
                           rdfs:label "series has publisher"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#setIn
ligenco:setIn rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain ligenco:Publisher ;
              rdfs:range ligenco:Country ;
              rdfs:comment "A publisher is located in a country."@en ;
              rdfs:label "set in"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#writtenBy
ligenco:writtenBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf ligenco:wrote ;
                  rdfs:domain ligenco:Book ;
                  rdfs:range ligenco:Writer ;
                  rdfs:comment "A book has been written by a writer."@en ;
                  rdfs:label "written by"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#wrote
ligenco:wrote rdf:type owl:ObjectProperty ;
              rdfs:domain ligenco:Writer ;
              rdfs:range ligenco:Book ;
              rdfs:comment "A writer wrote a book."@en ;
              rdfs:label "wrote"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#birth_date
ligenco:birth_date rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain ligenco:Writer ;
                   rdfs:range xsd:dateTime ;
                   rdfs:comment "Writer's birth date."@en ;
                   rdfs:label "birth date"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#country_name
ligenco:country_name rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain ligenco:Country ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Name which a country is identified by."@en ;
                     rdfs:label "country name"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#death_date
ligenco:death_date rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain ligenco:Writer ;
                   rdfs:range xsd:dateTime ;
                   rdfs:comment "Writer's death date."@en ;
                   rdfs:label "death date"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#edition_date
ligenco:edition_date rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain ligenco:BookEdition ;
                     rdfs:range xsd:dateTime ;
                     rdfs:comment "Book edition's publication date."@en ;
                     rdfs:label "edition date"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#edition_language
ligenco:edition_language rdf:type owl:DatatypeProperty ;
                         rdfs:domain ligenco:BookEdition ;
                         rdfs:range xsd:string ;
                         rdfs:comment "Language under which book edition has been published (it can be different from the original book language)."@en ;
                         rdfs:label "edition language"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#edition_title
ligenco:edition_title rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain ligenco:BookEdition ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Title under which book edition has been published."@en ;
                      rdfs:label "edition title"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#end_date
ligenco:end_date rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain ligenco:LiteraryMovement ;
                 rdfs:range xsd:nonNegativeInteger ;
                 rdfs:comment "Literary movement end date"@en ;
                 rdfs:label "end date"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#original_date
ligenco:original_date rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain ligenco:Book ;
                      rdfs:range xsd:nonNegativeInteger ;
                      rdfs:comment "Original book's publication date."@en ;
                      rdfs:label "original date"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#original_language
ligenco:original_language rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain ligenco:Book ;
                          rdfs:range xsd:string ;
                          rdfs:comment "Language under which the original book has been published."@en ;
                          rdfs:label "original language"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#original_title
ligenco:original_title rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain ligenco:Book ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Title under which the original book has been published."@en ;
                       rdfs:label "original title"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publisher_name
ligenco:publisher_name rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain ligenco:Publisher ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Name which a publisher is identified by."@en ;
                       rdfs:label "publisher name"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#start_date
ligenco:start_date rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain ligenco:LiteraryMovement ;
                   rdfs:range xsd:nonNegativeInteger ;
                   rdfs:comment "Literary movement start date"@en ;
                   rdfs:label "start date"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#viaf_id
ligenco:viaf_id rdf:type owl:DatatypeProperty ;
                rdfs:domain ligenco:Writer ;
                rdfs:range xsd:string ;
                rdfs:comment "Writer's VIAF ID."@en ;
                rdfs:label "viaf id"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#writer_name
ligenco:writer_name rdf:type owl:DatatypeProperty ;
                    owl:equivalentProperty foaf:name ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain ligenco:Writer ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Name which a writer is identified by."@en ;
                    rdfs:label "writer name"@en .


###  http://xmlns.com/foaf/0.1/#term_name
foaf:name rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:comment "A name for some thing"@en ;
          rdfs:isDefinedBy "http://xmlns.com/foaf/0.1/" ;
          rdfs:label "name"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/spar/fabio/Biography
fabio:Biography rdf:type owl:Class ;
                owl:equivalentClass ligenco:Biography ;
                rdfs:subClassOf ligenco:Narrative ;
                rdfs:comment "An account of the events, works and achievements, both personal and professional, of a person, either living or dead."@en ;
                rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
                rdfs:label "biography"@en .


###  http://purl.org/spar/fabio/Book
fabio:Book rdf:type owl:Class ;
           owl:equivalentClass ligenco:BookEdition ;
           rdfs:comment "A non-serial document that is complete in one volume or a designated finite number of volumes. A book published by a publisher is usually identified by an International Standard Book Number (ISBN), and may be manifested as a physical printed publication on paper bound in a hard or soft cover, or in electronic format as an 'e-book'."@en ;
           rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
           rdfs:label "book"@en .


###  http://purl.org/spar/fabio/BookSeries
fabio:BookSeries rdf:type owl:Class ;
                 owl:equivalentClass ligenco:BookCollection ;
                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> ;
                 rdfs:comment "A sequence of books having certain characteristics in common that are formally identified together as a group - for instance, the books in the Law, Governance and Technology Series published by Springer."@en ;
                 rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
                 rdfs:label "book series"@en .


###  http://purl.org/spar/fabio/BookSet
fabio:BookSet rdf:type owl:Class ;
              owl:equivalentClass ligenco:BookSeries ;
              rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
              rdfs:comment "A set of books having certain characteristics in common that informally allow their identification together as a group - for instance, the books of the Harry Potter saga."@en ;
              rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
              rdfs:label "book set"@en .


###  http://purl.org/spar/fabio/Diary
fabio:Diary rdf:type owl:Class ;
            owl:equivalentClass ligenco:Diary ;
            rdfs:subClassOf ligenco:Narrative ;
            rdfs:comment "A personal record, in a form of book, with discrete entries (often handwritten) arranged by date, reporting what has happened over the course of a day or other period of time."@en ;
            rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
            rdfs:label "diary"@en .


###  http://purl.org/spar/fabio/Essay
fabio:Essay rdf:type owl:Class ;
            owl:equivalentClass ligenco:Essay ;
            rdfs:subClassOf ligenco:Divulgative ;
            rdfs:comment "A piece of non-fiction writing on a particular subject, usually of moderate length and without chapters."@en ;
            rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
            rdfs:label "essay"@en .


###  http://purl.org/spar/fabio/LiteraryArtisticWork
fabio:LiteraryArtisticWork rdf:type owl:Class ;
                           rdfs:comment "A literary creative work, such as a novel, play, poem or song."@en ;
                           rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
                           rdfs:label "literary artistic work"@en .


###  http://purl.org/spar/fabio/Novel
fabio:Novel rdf:type owl:Class ;
            owl:equivalentClass ligenco:Romance ;
            rdfs:subClassOf ligenco:Narrative ;
            rdfs:comment "A long fictitious narrative written in literary prose."@en ;
            rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
            rdfs:label "novel"@en .


###  http://purl.org/spar/fabio/Play
fabio:Play rdf:type owl:Class ;
           owl:equivalentClass ligenco:Drama ;
           rdfs:subClassOf ligenco:Book ;
           rdfs:comment "A form of literature written by a playwright, usually consisting of scripted dialogue between characters, intended for theatrical performance rather than reading."@en ;
           rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
           rdfs:label "play"@en .


###  http://purl.org/spar/fabio/Poem
fabio:Poem rdf:type owl:Class ;
           rdfs:subClassOf ligenco:Poetry ;
           rdfs:comment "An artistic work written with an intensity or beauty of language more characteristic of poetry than of prose."@en ;
           rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
           rdfs:label "poem"@en .


###  http://purl.org/spar/fabio/ShortStory
fabio:ShortStory rdf:type owl:Class ;
                 owl:equivalentClass ligenco:ShortStories ;
                 rdfs:subClassOf ligenco:Narrative ;
                 rdfs:comment "A work of fiction that is usually written in prose, often in narrative format. This format tends to be more focused and less elaborate than longer works of fiction such as novels."@en ;
                 rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
                 rdfs:label "short story"@en .


###  http://purl.org/spar/fabio/Song
fabio:Song rdf:type owl:Class ;
           owl:equivalentClass ligenco:Song ;
           rdfs:subClassOf ligenco:LyricPoetry ;
           rdfs:comment "A musical composition that contains vocal parts ('lyrics') that are performed ('sung')."@en ;
           rdfs:isDefinedBy "http://purl.org/spar/fabio/" ;
           rdfs:label "song"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#AdventureRomance
ligenco:AdventureRomance rdf:type owl:Class ;
                         rdfs:subClassOf ligenco:Romance ;
                         rdfs:comment "Type of fiction that usually presents danger, or gives the reader a sense of excitement."@en ;
                         rdfs:label "Adventure romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#AncientBook
ligenco:AncientBook rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( ligenco:Book
                                                               [ rdf:type owl:Class ;
                                                                 owl:complementOf ligenco:ModernBook
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf ligenco:Book ;
                    owl:disjointWith ligenco:ModernBook ;
                    rdfs:comment "A book written and published before 1830."@en ;
                    rdfs:label "Ancient book"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Aphorisms
ligenco:Aphorisms rdf:type owl:Class ;
                  rdfs:subClassOf ligenco:Divulgative ;
                  rdfs:comment "A concise, terse, laconic or memorable expression of a general truth or principle."@en ;
                  rdfs:label "Aphorisms"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Ballata
ligenco:Ballata rdf:type owl:Class ;
                rdfs:subClassOf ligenco:LyricPoetry ;
                rdfs:comment "Italian poetry with the musical structure AbbaA"@en ;
                rdfs:label "Ballata"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Bildungsromance
ligenco:Bildungsromance rdf:type owl:Class ;
                        rdfs:subClassOf ligenco:Romance ;
                        rdfs:comment "A novel dealing with one person's formative years or spiritual education."@en ;
                        rdfs:label "Bildungsromance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BilingualWriter
ligenco:BilingualWriter rdf:type owl:Class ;
                        rdfs:subClassOf ligenco:Writer ;
                        rdfs:comment "A writer that wrote in more than two languages"@en ;
                        rdfs:label "Bilingual writer"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Biography
ligenco:Biography rdf:type owl:Class ;
                  rdfs:subClassOf ligenco:Narrative ;
                  rdfs:comment "Detailed description of a person life."@en ;
                  rdfs:label "Biography"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book
ligenco:Book rdf:type owl:Class ;
             owl:equivalentClass gnd:Manuscript ;
             rdfs:subClassOf fabio:LiteraryArtisticWork ;
             rdfs:comment "Medium for information recording (digital or on paper) composed of may pages."@en ;
             rdfs:label "Book"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BookCollection
ligenco:BookCollection rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> ;
                       owl:disjointWith ligenco:BookSeries ;
                       rdfs:comment "A collection of books grouped by genre or aruthor and belonging to a specific publisher."@en ;
                       rdfs:label "Book collection"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BookEdition
ligenco:BookEdition rdf:type owl:Class ;
                    rdfs:comment "Book version distributed by a specific publisher."@en ;
                    rdfs:label "Book edition"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BookSeries
ligenco:BookSeries rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
                   rdfs:comment "An ordered collection of books related to the same plot."@en ;
                   rdfs:label "Book series"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#ChameleonicWriter
ligenco:ChameleonicWriter rdf:type owl:Class ;
                          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                       owl:onProperty ligenco:wrote ;
                                                                       owl:someValuesFrom ligenco:Drama
                                                                     ]
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty ligenco:wrote ;
                                                                       owl:someValuesFrom ligenco:Poetry
                                                                     ]
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty ligenco:wrote ;
                                                                       owl:someValuesFrom ligenco:Prose
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ] ;
                          rdfs:subClassOf ligenco:Writer ;
                          rdfs:comment "Writer which wrote books of every type: drama, poetry and prose."@en ;
                          rdfs:label "Chameleonic writer"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#ChildrenRomance
ligenco:ChildrenRomance rdf:type owl:Class ;
                        rdfs:subClassOf ligenco:Romance ;
                        rdfs:comment "Novel written for children"@en ;
                        rdfs:label "Children romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Comedy
ligenco:Comedy rdf:type owl:Class ;
               rdfs:subClassOf ligenco:Drama ;
               owl:disjointWith ligenco:Tragedy ;
               rdfs:comment "Dramatic works that combines elements of comedy and drama."@en ;
               rdfs:label "Comedy"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#ComedyRomance
ligenco:ComedyRomance rdf:type owl:Class ;
                      rdfs:subClassOf ligenco:Romance ;
                      rdfs:comment "Type of comedy and slice-of-life fiction, focusing on lighthearted, humorous plot lines centered on romantic ideas, such as how true love is able to surmount most obstacles."@en ;
                      rdfs:label "Comedy romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Country
ligenco:Country rdf:type owl:Class ;
                rdfs:subClassOf ligenco:GeographicalArea ;
                rdfs:comment "Nation with its own government occupying a particular territory."@en ;
                rdfs:label "Country"@en .


