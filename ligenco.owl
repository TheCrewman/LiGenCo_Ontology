ligenco:CrimefictionRomance rdf:type owl:Class ;
                            rdfs:subClassOf ligenco:Romance ;
                            rdfs:comment "Narratives centred on criminal acts and especially on the investigation."@en ;
                            rdfs:label "Crimefiction romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#DetectivefictionRomance
ligenco:DetectivefictionRomance rdf:type owl:Class ;
                                rdfs:subClassOf ligenco:CrimefictionRomance ;
                                owl:disjointWith ligenco:NoirRomance ;
                                rdfs:comment "Novel in which an investigator or a detective investigates a crime, often murder."@en ;
                                rdfs:label "Detectivefiction romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Dialogue
ligenco:Dialogue rdf:type owl:Class ;
                 rdfs:subClassOf ligenco:Divulgative ;
                 rdfs:comment "Transcript of a conversation between two or more speakers."@en ;
                 rdfs:label "Dialogue"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Diary
ligenco:Diary rdf:type owl:Class ;
              rdfs:subClassOf ligenco:Narrative ;
              rdfs:comment "Written record with discrete entries arranged by date reporting on what happened during a day or other period."@en ;
              rdfs:label "Diary"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#DidacticPoetry
ligenco:DidacticPoetry rdf:type owl:Class ;
                       rdfs:subClassOf ligenco:Poetry ;
                       rdfs:label "Didactic poetry"@en ;
                       owl:versionInfo "Poetry that instructs, either in terms of morals or by providing knowledge."@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Divulgative
ligenco:Divulgative rdf:type owl:Class ;
                    rdfs:subClassOf ligenco:Prose ;
                    owl:disjointWith ligenco:Narrative ;
                    rdfs:comment "Written text with the inteded mean of spreading ideas or specific knowledge."@en ;
                    rdfs:label "Divulgative prose"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Drama
ligenco:Drama rdf:type owl:Class ;
              rdfs:subClassOf ligenco:Book ;
              rdfs:comment "Book describing the setting and plot of a show which will take place on a stage."@en ;
              rdfs:label "Drama"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#EroticRomance
ligenco:EroticRomance rdf:type owl:Class ;
                      rdfs:subClassOf ligenco:Romance ;
                      rdfs:comment "Written stories about the development of a romantic relationship through sexual interaction."@en ;
                      rdfs:label "Erotic romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Essay
ligenco:Essay rdf:type owl:Class ;
              rdfs:subClassOf ligenco:Divulgative ;
              rdfs:comment "Piece of writing that give the author's own argument about a specific topic."@en ;
              rdfs:label "Essay"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Fable
ligenco:Fable rdf:type owl:Class ;
              rdfs:subClassOf ligenco:Narrative ;
              rdfs:comment "Prose that features animals, legendary creatures, etc. that illustrates o leads to a particular moral lesson."@en ;
              rdfs:label "Fable"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#FantahistoricalRomance
ligenco:FantahistoricalRomance rdf:type owl:Class ;
                               rdfs:subClassOf ligenco:HistoricalRomance ;
                               rdfs:comment "Historical fiction that incorporates fantastic elements, such as magic, into a more realistic narrative."@en ;
                               rdfs:label "Fantahistorical romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#FantasyRomance
ligenco:FantasyRomance rdf:type owl:Class ;
                       rdfs:subClassOf ligenco:Romance ;
                       rdfs:comment "Novel set in an imaginary universe, often but not always without any locations, events, or people from the real world."@en ;
                       rdfs:label "Fantasy romance"@en .


