###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#HistoricalRomance
ligenco:HistoricalRomance rdf:type owl:Class ;
                          rdfs:subClassOf ligenco:Romance ;
                          rdfs:comment "Fictional text in which the plot takes place in a setting related to past events."@en ;
                          rdfs:label "Historical romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#HorrorRomance
ligenco:HorrorRomance rdf:type owl:Class ;
                      rdfs:subClassOf ligenco:Romance ;
                      rdfs:comment "Fiction in prose of variable length which shocks, or even frightens, the reader or perhaps induces a feeling of repulsion."@en ;
                      rdfs:label "Horror romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#IndividualBook
ligenco:IndividualBook rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> ;
                       owl:disjointWith ligenco:SequenceBook ;
                       rdfs:comment "Book part of a book collection."@en ;
                       rdfs:label "Individual book"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#LiteraryMovement
ligenco:LiteraryMovement rdf:type owl:Class ;
                         rdfs:comment "Period of time mainly characterized by a particular literary genre."@en ;
                         rdfs:label "Literary movement"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#LyricPoetry
ligenco:LyricPoetry rdf:type owl:Class ;
                    rdfs:subClassOf ligenco:Poetry ;
                    rdfs:comment "Poetry wich express personal emotions or feelings."@en ;
                    rdfs:label "Lyric poetry"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Madrigal
ligenco:Madrigal rdf:type owl:Class ;
                 rdfs:subClassOf ligenco:LyricPoetry ;
                 rdfs:comment "Secular vocal music composition"@en ;
                 rdfs:label "Madrigal"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Manifest
ligenco:Manifest rdf:type owl:Class ;
                 rdfs:subClassOf ligenco:Divulgative ;
                 rdfs:comment "A published declaration of the intentions, motives or views of the issuer."@en ;
                 rdfs:label "Manifest"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#ModernBook
ligenco:ModernBook rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty ligenco:original_date ;
                                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                              owl:onDatatype xsd:nonNegativeInteger ;
                                                              owl:withRestrictions ( [ xsd:minExclusive "1830"^^xsd:nonNegativeInteger
                                                                                     ]
                                                                                   )
                                                            ]
                                       ] ;
                   rdfs:subClassOf ligenco:Book ;
                   rdfs:comment "A book written and published after 1830."@en ;
                   rdfs:label "Modern book"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Narrative
ligenco:Narrative rdf:type owl:Class ;
                  rdfs:subClassOf ligenco:Prose ;
                  rdfs:comment "Any account of a series of related event or experience, whether non-fictional or fictional."@en ;
                  rdfs:label "Narrative"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#NoirRomance
ligenco:NoirRomance rdf:type owl:Class ;
                    rdfs:subClassOf ligenco:CrimefictionRomance ;
                    rdfs:comment "Novel marked by darkness in theme and subject matter, generally featuring a disturbing mixture of sex and violence."@en ;
                    rdfs:label "Noir romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Playwright
ligenco:Playwright rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty ligenco:wrote ;
                                         owl:someValuesFrom ligenco:Drama
                                       ] ;
                   rdfs:subClassOf ligenco:Writer ;
                   rdfs:comment "Writer who has written at least one drama."@en ;
                   rdfs:label "Playwright"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Poet
ligenco:Poet rdf:type owl:Class ;
             owl:equivalentClass gnd:poet ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ligenco:wrote ;
                                   owl:someValuesFrom ligenco:Poetry
                                 ] ;
             rdfs:subClassOf ligenco:Writer ;
             rdfs:comment "Writer who has written at least one poetry."@en ;
             rdfs:label "Poet"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Poetry
ligenco:Poetry rdf:type owl:Class ;
               rdfs:subClassOf ligenco:Book ;
               rdfs:comment "Literary composition written by a poet that uses aesthetic and rhyhmic qualities of language."@en ;
               rdfs:label "Poetry"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Prose
ligenco:Prose rdf:type owl:Class ;
              rdfs:subClassOf ligenco:Book ;
              rdfs:comment "Written text in his ordinary form without metrical structure."@en ;
              rdfs:label "Prose"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#ProseWriter
ligenco:ProseWriter rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty ligenco:wrote ;
                                          owl:someValuesFrom ligenco:Prose
                                        ] ;
                    rdfs:subClassOf ligenco:Writer ;
                    rdfs:comment "Writer who has written at least a prose."@en ;
                    rdfs:label "Prose writer"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Publisher
ligenco:Publisher rdf:type owl:Class ;
                  rdfs:comment "Entity which publish and distribute a book."@en ;
                  rdfs:label "Publisher"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Romance
ligenco:Romance rdf:type owl:Class ;
                rdfs:subClassOf ligenco:Narrative ;
                rdfs:comment "Relatively long work of narrative fiction."@en ;
                rdfs:label "Romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#RomanticRomance
ligenco:RomanticRomance rdf:type owl:Class ;
                        rdfs:subClassOf ligenco:Romance ;
                        rdfs:comment "Novel which places its primary focus on the relationship and romantic love between two people."@en ;
                        rdfs:label "Romantic romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#SciencefictionRomance
ligenco:SciencefictionRomance rdf:type owl:Class ;
                              rdfs:subClassOf ligenco:Romance ;
                              rdfs:comment "Novel which typically deals with imaginative and futuristic concepts."@en ;
                              rdfs:label "Sciencefiction romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#SequenceBook
ligenco:SequenceBook rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                     rdfs:comment "Book part of a book series."@en ;
                     rdfs:label "Sequence book"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#ShortStories
ligenco:ShortStories rdf:type owl:Class ;
                     rdfs:subClassOf ligenco:Narrative ;
                     rdfs:comment "Prose fiction that typically can be read in one sitting and focuses on a self-contained incident or series of linked incidents, with the intent of evoking a single effect or mood."@en ;
                     rdfs:label "Short stories"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Song
ligenco:Song rdf:type owl:Class ;
             rdfs:subClassOf ligenco:LyricPoetry ;
             rdfs:comment "Musical composition accompained by specific lyrics intended to be performed by the human voice."@en ;
             rdfs:label "Song"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Sonnet
ligenco:Sonnet rdf:type owl:Class ;
               rdfs:subClassOf ligenco:LyricPoetry ;
               rdfs:comment "Poetry composition of 14 lines which follows a strict rhyme sheme."@en ;
               rdfs:label "Sonnet"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#ThrillerRomance
ligenco:ThrillerRomance rdf:type owl:Class ;
                        rdfs:subClassOf ligenco:Romance ;
                        rdfs:comment "Novel characterized by feelings of suspense, excitement, surprise, anticipation and anxiety."@en ;
                        rdfs:label "Thriller romance"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Tragedy
ligenco:Tragedy rdf:type owl:Class ;
                rdfs:subClassOf ligenco:Drama ;
                rdfs:comment "Drama based on human suffering and, mainly, the terrible or sorrowful events that befall a main character."@en ;
                rdfs:label "Tragedy"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#TranslatedBook
ligenco:TranslatedBook rdf:type owl:Class ;
                       rdfs:subClassOf ligenco:Book ;
                       rdfs:comment "A book that has been translated in at least a language"@en ;
                       rdfs:label "Translated book"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Treatise
ligenco:Treatise rdf:type owl:Class ;
                 rdfs:subClassOf ligenco:Divulgative ;
                 rdfs:comment "A written work dealing formaly or systematically with a subject."@en ;
                 rdfs:label "Treatise"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#WandovagoWriter
ligenco:WandovagoWriter rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty ligenco:livedIn ;
                                              owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                              owl:onClass ligenco:Country
                                            ] ;
                        rdfs:subClassOf ligenco:Writer ;
                        rdfs:comment "Writer who has lived in at least 2 country."@en ;
                        rdfs:label "Wandovago writer"@en .


###  http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer
ligenco:Writer rdf:type owl:Class ;
               owl:equivalentClass gnd:author ;
               rdfs:subClassOf foaf:Person ;
               rdfs:comment "Person which writes books."@en ;
               rdfs:label "Writer"@en .


###  http://xmlns.com/foaf/0.1/#term_Person
foaf:Person rdf:type owl:Class ;
            rdfs:comment "A person"@en ;
            rdfs:isDefinedBy "http://xmlns.com/foaf/0.1/" ;
            rdfs:label "Person"@en .


###  https://d-nb.info/standards/elementset/gnd#Manuscript
gnd:Manuscript rdf:type owl:Class ;
               rdfs:subClassOf fabio:LiteraryArtisticWork ;
               rdfs:isDefinedBy "https://d-nb.info/standards/elementset/gnd" ;
               rdfs:label "Manuscript"@en ,
                          "Schriftdenkmal"@de .


###  https://d-nb.info/standards/elementset/gnd#author
gnd:author rdf:type owl:Class ;
           rdfs:subClassOf foaf:Person ;
           rdfs:comment "A person, family, or organization responsible for creating a work that is primarily textual in content, regardless of media type (e.g., printed text, spoken word, electronic text, tactile text) or genre (e.g., poems, novels, screenplays, blogs). Use also for persons, etc., creating a new work by paraphrasing, rewriting, or adapting works by another creator such that the modification has substantially changed the nature and content of the original or changed the medium of expression."@en ;
           rdfs:isDefinedBy "https://d-nb.info/standards/elementset/gnd" ;
           rdfs:label "Author"@en ,
                      "Verfasser"@de .


###  https://d-nb.info/standards/elementset/gnd#poet
gnd:poet rdf:type owl:Class ;
         rdfs:subClassOf ligenco:Writer ;
         rdfs:comment "An author of the words of a non-dramatic musical work (e.g. the text of a song), except for oratorios"@en ;
         rdfs:isDefinedBy "https://d-nb.info/standards/elementset/gnd" ;
         rdfs:label "Dichter"@de ,
                    "Poet"@en .


