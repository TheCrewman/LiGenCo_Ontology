                                                           ligenco:North_America
                                                           ligenco:North_Asia
                                                           ligenco:North_Europe
                                                           ligenco:Oceania
                                                           ligenco:Polynesia
                                                           ligenco:South_America
                                                           ligenco:South_Asia
                                                           ligenco:South_East_Asia
                                                           ligenco:South_Europe
                                                           ligenco:Sub-Saharan_Africa
                                                           ligenco:Western_Asia
                                                           ligenco:Western_Europe
