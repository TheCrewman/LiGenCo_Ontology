<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#"
     xml:base="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco"
     xmlns:dc="http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#"
     xmlns:bag="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:ligenco="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco">
        <owl:imports rdf:resource="http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/list.owl"/>
        <dc:creator xml:lang="en">Daniele Liberatore</dc:creator>
        <dc:creator xml:lang="en">Matteo Palazzo</dc:creator>
        <dc:date>2022-09-13</dc:date>
        <dc:description xml:lang="en">An ontology used to describe the distribution and production of book genres across the world.</dc:description>
        <dc:title xml:lang="en">Literary Genres by Country</dc:title>
        <vann:preferredNamespacePrefix>ligenco</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#</vann:preferredNamespaceUri>
        <rdfs:label xml:lang="en">LiGenCo</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix">
        <rdfs:comment xml:lang="en">The preferred namespace prefix to use when using terms from this vocabulary in an XML document.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/vocab/vann/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Preferred Namespace Prefix</rdfs:label>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri">
        <rdfs:comment xml:lang="en">The preferred namespace URI to use when using terms from this vocabulary in an XML document.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/vocab/vann/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Preferred Namespace Uri</rdfs:label>
    </owl:AnnotationProperty>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent"/>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#belongToMovement -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#belongToMovement">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#LiteraryMovement"/>
        <rdfs:comment xml:lang="en">A writer is born in a specific epoch related to a major literary movement.</rdfs:comment>
        <rdfs:label xml:lang="en">belong to movement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasBeenPublished -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasBeenPublished">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BookEdition"/>
        <rdfs:comment xml:lang="en">A book has been published as a specific book edition.</rdfs:comment>
        <rdfs:label xml:lang="en">has been published</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasBeenPublishedInCountry -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasBeenPublishedInCountry">
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasBeenPublished"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publishedIn"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">A book has been published as a specific book edition in a particular country.</rdfs:comment>
        <rdfs:label xml:lang="en">has been published in country</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasCollection -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasCollection">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasPublisher"/>
        <rdfs:comment xml:lang="en">A publisher which published a book collection.</rdfs:comment>
        <rdfs:label xml:lang="en">has collection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasPublisher -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#hasPublisher">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BookCollection"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Publisher"/>
        <rdfs:comment xml:lang="en">A book collection published by a publisher.</rdfs:comment>
        <rdfs:label xml:lang="en">has publisher</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#isCoevalTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#isCoevalTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#isCompatriotWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#isCompatriotWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#livedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#livedIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Country"/>
        <rdfs:comment xml:lang="en">A writer lived in a country.</rdfs:comment>
        <rdfs:label xml:lang="en">lived in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#livedInTheSameEpochAs -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#livedInTheSameEpochAs">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#locatedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#locatedIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#GeographicalArea"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#GeographicalArea"/>
        <rdfs:comment xml:lang="en">A geographical area can be located into an another and more restricted geographical area.</rdfs:comment>
        <rdfs:label xml:lang="en">located in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#overlapsWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#overlapsWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#LiteraryMovement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#LiteraryMovement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publishedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publishedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BookEdition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Publisher"/>
        <rdfs:comment xml:lang="en">A book edition is published by a publisher.</rdfs:comment>
        <rdfs:label xml:lang="en">published by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publishedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publishedIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BookEdition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Country"/>
        <rdfs:comment xml:lang="en">A book edition has been published in a country.</rdfs:comment>
        <rdfs:label xml:lang="en">published in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#relatedTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#relatedTo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Country"/>
        <rdfs:comment xml:lang="en">A writer is related to a country if he is born in this country or if he is an active citizen of it.</rdfs:comment>
        <rdfs:label xml:lang="en">related to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#relatedToIndividualBook -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#relatedToIndividualBook">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#IndividualBook"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book"/>
        <rdfs:label xml:lang="en">related to individual book</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#relatedToSequenceBook -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#relatedToSequenceBook">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#SequenceBook"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#setIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#setIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Publisher"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Country"/>
        <rdfs:comment xml:lang="en">A publisher is located in a country.</rdfs:comment>
        <rdfs:label xml:lang="en">set in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#wrote -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#wrote">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#wroteBy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book"/>
        <rdfs:comment xml:lang="en">A writer wrote a book.</rdfs:comment>
        <rdfs:label xml:lang="en">wrote</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#wroteBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#wroteBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
        <rdfs:comment xml:lang="en">A book has been writter by a writer.</rdfs:comment>
        <rdfs:label xml:lang="en">wrote by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#birth_date -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#birth_date">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Writer&apos;s birth date.</rdfs:comment>
        <rdfs:label xml:lang="en">birth date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#country_name -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#country_name">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Country"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Name which a country is identified by.</rdfs:comment>
        <rdfs:label xml:lang="en">country name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#death_date -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#death_date">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Writer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Writer&apos;s death date.</rdfs:comment>
        <rdfs:label xml:lang="en">death date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#edition_date -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#edition_date">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BookEdition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Book edition&apos;s publication date.</rdfs:comment>
        <rdfs:label xml:lang="en">edition date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#edition_language -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#edition_language">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BookEdition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Language under which book edition has been published (it can be different from the original book language).</rdfs:comment>
        <rdfs:label xml:lang="en">edition language</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#edition_title -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#edition_title">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#BookEdition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Title under which book edition has been published.</rdfs:comment>
        <rdfs:label xml:lang="en">edition title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#end_date -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#end_date">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#LiteraryMovement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#original_date -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#original_date">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment xml:lang="en">Original book&apos;s publication date.</rdfs:comment>
        <rdfs:label xml:lang="en">original date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#original_language -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#original_language">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Language under which the original book has been published.</rdfs:comment>
        <rdfs:label xml:lang="en">original language</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#original_title -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#original_title">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#Book"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Title under which the original book has been published.</rdfs:comment>
        <rdfs:label xml:lang="en">original title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publisher_name -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/daniele/ontologies/2022/5/ligenco#publisher_name">
